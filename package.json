{
  "name": "zx-electron",
  "version": "0.0.1",
  "description": "Angular + Electron",
  "homepage": "https://github.com/oncelxlc/zx-electron",
  "bugs": {
    "url": "https://github.com/oncelxlc/zx-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:angular": "ng build --watch --configuration development",
    "dev:electron": "wait-on http://localhost:9701 && electron .",
    "dev:serve": "npm-run-all -p start dev:electron",
    "dev": "npm-run-all -p dev:angular dev:electron",
    "electron": "electron ."
  },
  "engines": {
    "node": "^20.18.1 || >22.12.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of YARN to install dependencies",
    "pnpm": "Please use yarn instead of YARN to install dependencies"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/router": "^19.0.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
