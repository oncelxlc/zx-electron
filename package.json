{
  "name": "zx-electron",
  "version": "0.0.1",
  "description": "Angular + Electron",
  "homepage": "https://github.com/oncelxlc/zx-electron",
  "bugs": {
    "url": "https://github.com/oncelxlc/zx-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev-build:angular": "ng build --watch --configuration development",
    "dev:electron": "wait-on http://localhost:9701 && electron .",
    "dev-build:electron": "wait-on ./dist/zx-electron/browser/index.html && electron .",
    "dev:serve": "concurrently --kill-others \"npm run start\" \"npm run dev:electron\"",
    "dev:build": "concurrently dev-build:angular dev:electron",
    "electron": "electron ."
  },
  "engines": {
    "node": "^20.18.1 || >22.12.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of YARN to install dependencies",
    "pnpm": "Please use yarn instead of YARN to install dependencies"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.2",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^22.10.7",
    "concurrently": "^9.1.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.6.3",
    "wait-on": "^8.0.1"
  }
}
